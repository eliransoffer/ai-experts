t\n","word_index = tokenizer.word_index\n","vocab_size = len(word_index) + 1 # Add 1 for the reserved 0 padding index\n","print(\"Vocabulary:\")\n","print(word_index)\n","print(f\"Vocabulary Size: {vocab_size}\\n\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"LjsNUMGbT5Rr","executionInfo":{"status":"ok","timestamp":1753341748326,"user_tz":-180,"elapsed":60,"user":{"displayName":"Valeria Aynbinder","userId":"10876993785060579075"}},"outputId":"cae11079-7cbf-4a01-ef12-28c54e6d57fa"},"execution_count":7,"outputs":[{"output_type":"stream","name":"stdout","text":["Vocabulary:\n","{'<unk>': 1, 'work': 2, 'done': 3, 'good': 4, 'effort': 5, 'poor': 6, 'well': 7, 'great': 8, 'nice': 9, 'excellent': 10, 'amazing': 11, 'job': 12, 'fantastic': 13, 'result': 14, 'superb': 15, 'performance': 16, 'weak': 17, 'not': 18, 'could': 19, 'have': 20, 'better': 21, 'terrible': 22, 'outcome': 23, 'very': 24, 'bad': 25, 'awful': 26, 'experience': 27}\n","Vocabulary Size: 28\n","\n"]}]},{"cell_type":"code","source":["# Integer encode the documents\n","sequences = tokenizer.texts_to_sequences(docs)\n","print(\"Integer Encoded Sequences:\")\n","print(sequences)\n","print(\"\\n\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"nU7ONhb9UClb","executionInfo":{"status":"ok","timestamp":1753341773887,"user_tz":-180,"elapsed":59,"user":{"displayName":"Valeria Aynbinder","userId":"10876993785060579075"}},"outputId":"8e386993-044b-4e0e-8fec-f2c2b3a7666f"},"execution_count":8,"outputs":[{"output_type":"stream","name":"stdout","text":["Integer Encoded Sequences:\n","[[7, 3], [4, 2], [8, 5], [9, 2], [10], [11, 12], [13, 14], [15, 16], [17], [6, 5], [18, 4], [6, 2], [19, 20, 3, 21], [22, 23], [24, 25], [26, 27]]\n","\n","\n"]}]},{"cell_type":"code","source":["# Pad the sequences to ensure they all have the same length\n","max_length = 4\n","padded_sequences = pad_sequences(sequences, maxlen=max_length, padding='post')\n","print(\"Padded Sequences (max_length=4):\