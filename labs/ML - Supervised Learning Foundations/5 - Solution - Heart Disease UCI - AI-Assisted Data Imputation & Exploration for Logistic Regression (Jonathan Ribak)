","\n","        # Downcast integers\n","        if pd.api.types.is_integer_dtype(df[col]):\n","            if np.iinfo(np.int8).min <= col_min <= np.iinfo(np.int8).max:\n","                df[col] = df[col].astype(np.int8)\n","            elif np.iinfo(np.int16).min <= col_min <= np.iinfo(np.int16).max:\n","                df[col] = df[col].astype(np.int16)\n","            elif np.iinfo(np.int32).min <= col_min <= np.iinfo(np.int32).max:\n","                df[col] = df[col].astype(np.int32)\n","            # Else, keep as int64 (default)\n","\n","        # Downcast floats\n","        elif pd.api.types.is_float_dtype(df[col]):\n","            if df[col].dtype == np.float64:\n","                df[col] = df[col].astype(np.float32)\n","                if df[col].min() >= np.finfo(np.float16).min and df[col].max() <= np.finfo(np.float16).max:\n","                    df[col] = df[col].astype(np.float16)  # Further reduce to float16 if possible\n","\n","    return df"],"metadata":{"id":"mMQOPIVK1k5G"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["reduced_df = reduce_numeric_dtype(df)\n"],"metadata":{"id":"VfxQj8tm2_fX"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# convert target col ('num') to bool\n","reduced_df['num'] = reduced_df['num'].astype(bool)"],"metadata":{"id":"1hwe8Rc63uRA"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["reduced_df.rename(columns={'num': 'has_heart_disease'}, inplace=True)\n","reduced_df.drop(columns=['id', 'dataset'], inplace=True)"],"metadata":{"id":"IRN4sA-P6HkO"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["reduced_df.info()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"SCvRlRRv6xVr","executionInfo":{"status":"ok","timestamp":1742714064226,"user_tz":-120,"elapsed":16,"user":{"displayName":"יונתן ריבק","userId":"00303261127772919371"}},"outputId":"15f6ec15-b73a-4643-f5b5-9272a33a5f9a"},"execution_count":null,"outputs":[{"output_t