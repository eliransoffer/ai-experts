rom copy import deepcopy\n","\n","def find_best_k(pipeline, X_train, y_train, X_val, y_val):\n","    max_k = X_train.shape[1]  # Maximum k is the number of features\n","    best_k = None\n","    best_r2 = -np.inf\n","    best_mae = np.inf\n","    results = []\n","\n","    # Identify the feature selection step dynamically\n","    feature_selector_name = None\n","    for name, step in pipeline.steps:\n","        if isinstance(step, SelectKBest):\n","            feature_selector_name = name\n","            break\n","\n","    for k in range(1, max_k + 1):\n","        # If pipeline has a feature selector, set k properly\n","        if feature_selector_name:\n","            pipeline.set_params(**{f'{feature_selector_name}__k': min(k, max_k)})\n","\n","        # Train model\n","        pipeline.fit(X_train, y_train)\n","\n","        # Predict on validation set\n","        y_val_pred = pipeline.predict(X_val)\n","\n","        # Compute metrics\n","        r2 = r2_score(y_val, y_val_pred)\n","        mae = metrics.mean_absolute_error(y_val, y_val_pred)\n","        non_zero_mask = y_val != 0  # Ignore zero values in MAPE\n","        mape = (abs(y_val[non_zero_mask] - y_val_pred[non_zero_mask]) / y_val[non_zero_mask]).mean() * 100\n","\n","        # Store results\n","        results.append((k, r2, mae, mape))\n","\n","        # Select best k based on R² first, then MAE for tie-breaking\n","        if (r2 > best_r2) or (r2 == best_r2 and mae < best_mae):\n","            best_r2 = r2\n","            best_mae = mae\n","            best_k = k\n","            best_metrics = (r2, mae, mape)\n","\n","    # Convert results to DataFrame\n","    results_df = pd.DataFrame(results, columns=['k', 'R² Score', 'MAE', 'MAPE (%)'])\n","\n","    return best_k, best_metrics, results_df\n"],"metadata":{"id":"iXZLMkuYXK60"},"execution_count":null,"outputs":[]},{"cell_type":"markdown","source":["### **1st attempt:**\n","####StandardScaler, PolyomialFeatures, FeatureSelector, Linear Regression\n","*comment for Valeria*: I placed scaler before polynomial features to avoid infinite values in selcted features."],"metadata":{"id":"lMLVpOesAbbA"}},{"cell_type":"markdown","source":["#### Training on k = 30"],"metadata":{"id":"SvmGSalyEBCa"}},{"cell_type":"code","source":["pipeline = Pipeline([\n","    ('scaler', StandardScaler()),\n","    ('poly', PolynomialFeatures(degree=2, include_bias=False)),\n","    ('selector', SelectKBest(score_func=f_regression, k=30)),\n","    ('regressor', LinearRegression())\n","])\n","\n","# Fit the pipeline\n","pipeline.fit(X_train, y_train)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":235},"id":"0kHyGJwAOCsr","executionInfo":{"status":"ok","timestamp":1742714073853,"user_tz":-120,"elapsed":634,"user":{"displayName":"יונתן ריבק","userId":"00303261127772919371"}},"outputId":"a7564a07-ff79-4574-c576-63f2e2e57af4"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["Pipeline(steps=[('scaler', StandardScaler()),\n","                ('poly', PolynomialFeatures(include_bias=False)),\n","                ('selector',\n","                 SelectKBest(k=30,\n","                             score_func=<function f_regression at 0x78ed54bad800>)),\n","                ('regressor', LinearRegression())])"],"text/html":["<style>#sk-container-id-1 {\n","  /* Definition of color scheme common for light and dark mode */\n","  --sklearn-color-text: #000;\n","  --sklearn-color-text-muted: #666;\n","  --sklearn-color-line: gray;\n","  /* Definition of color scheme for unfitted estimators */\n","  --sklearn-color-unfitted-level-0: #fff5e6;\n","  --sklearn-color-unfitted-level-1: #f6e4d2;\n","  --sklearn-color-unfitted-level-2: #ffe0b3;\n","  --sklearn-color-unfitted-level-3: chocolate;\n","  /* Definition of color scheme for fitted estimators */\n","  --sklearn-color-fitted-level-0: #f0f8ff;\n","  --sklearn-color-fitted-level-1: #d4ebff;\n","  --sklearn-color-fitted-level-2: #b3dbfd;\n","  --skle